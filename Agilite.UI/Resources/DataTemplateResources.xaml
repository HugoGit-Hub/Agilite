<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Agilite.UI.ViewModels"
                    xmlns:views="clr-namespace:Agilite.UI.Views">
    <DataTemplate DataType="{x:Type viewModels:DefaultViewModel}">
        <views:DefaultView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:TeamViewModel}">
        <views:TeamView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:SprintViewModel}">
        <views:SprintView />
    </DataTemplate>
</ResourceDictionary>