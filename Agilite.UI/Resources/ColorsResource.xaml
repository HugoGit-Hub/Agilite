<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrincipalColor">#2e2b3f</SolidColorBrush>
    <SolidColorBrush x:Key="BackgroundColor">#221f2e</SolidColorBrush>
    <SolidColorBrush x:Key="SecondColor">#567ad9</SolidColorBrush>
    <SolidColorBrush x:Key="TextColor">#efd9b4</SolidColorBrush>
    <LinearGradientBrush x:Key="Gradiant" StartPoint="0,1" EndPoint="1,0">
        <GradientStop Color="#d56ef0" Offset="0.20"/>
        <GradientStop Color="#567ad9" Offset="0.70"/>
    </LinearGradientBrush>
</ResourceDictionary>